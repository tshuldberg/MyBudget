{
  "expo": {
    "name": "MyBudget",
    "slug": "mybudget",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "scheme": "mybudget",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#1A1A2E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mybudget.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A1A2E"
      },
      "package": "com.mybudget.app"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-notifications"
    ]
  }
}
